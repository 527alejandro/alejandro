{
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {},
    "rootDir": ".",
    "sourceMap": true,
    "inlineSources": true,
    "lib": [
      "es6",
      "dom"
    ],
    "target": "es5",
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "bazelOptions": {
    "suppressTsconfigOverrideWarnings": true
  }
}
